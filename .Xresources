!! Colorscheme

! special
*.foreground: #93a1a1
*.background: [85]#1d2021
urxvt*depth: 32
!urxvt*transparent: true
!urxvt*shading: 15

*.cursorColor: #afbfbf

! black
*.color0: #263640
*.color8: #4a697d

! red
*.color1: #d12f2c
*.color9: #fa3935

! green
*.color2: #819400
*.color10: #a4bd00

! yellow
*.color3: #b08500
*.color11: #d9a400

! blue
*.color4: #2587cc
*.color12: #2ca2f5

! magenta
*.color5: #696ebf
*.color13: #8086e8

! cyan
*.color6: #289c93
*.color14: #33c5ba

! white
*.color7: #bfbaac
*.color15: #fdf6e3


URxvt.scrollBar: false
URxvt.cursorBlink: true
URxvt.internalBorder:20

URxvt.font: xft:Fira Code Retina:pixelsize=12,xft:PowerlineSymbols:pixelsize=12,xft
*.boldFont: xft:JetBrains Mono:style=Bold:size=10
*.italicFont: xft:JetBrains Mono:style=Italic:size=10
*.boldItalicFont: xft:JetBrains Mono:style=Bold Italic:size=10
